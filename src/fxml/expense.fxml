<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<AnchorPane prefHeight="600.0" prefWidth="800.0" stylesheets="@/styles.css"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.ExpenseController">
    <HBox>
        <TableView fx:id="table" prefHeight="600.0" prefWidth="300.0">
            <columns>
                <TableColumn fx:id="nameColumn" prefWidth="195.0" text="Name" />
                <TableColumn fx:id="amountColumn" prefWidth="100.0" text="Amount" />
            </columns>
        </TableView>

        <PieChart fx:id="chart" prefHeight="350.0" prefWidth="350.0">

        </PieChart>
    </HBox>
</AnchorPane>
